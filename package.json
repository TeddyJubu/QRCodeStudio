{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --config vitest.config.ts",
    "test:integration": "vitest --config vitest.config.ts --reporter=verbose",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "quality": "npm run lint && npm run format:check && npm run check",
    "prepare": "husky install",
    "hooks:install": "husky install",
    "ci:local": "npm run quality && npm run test:coverage && npm run build",
    "analyze": "npm run build && open dist/public/bundle-analysis.html",
    "audit:lighthouse": "lhci autorun",
    "audit:performance": "npm run audit:lighthouse && npm run analyze",
    "audit:full": "./scripts/audit-performance.sh"
  },
